<div class="container">
    <div class="row">
        <div class="col-6">
            <%= form_for [@community, @post] do |form| %>
            <div class="form-grouo">
                <%= label_tag :title %>
                <%= form.text_field :title, class: "form-control" %>
            </div>
            <div class="form-grouo">
                <%= label_tag :body %>
                <%= form.text_field :body, class: "form-control" %>
            </div>
            <%= form.submit "Submit", class: "btn btn-primary" %>

            <% end %>
        </div>
    </div>
</div>